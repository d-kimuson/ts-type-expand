{
  "name": "ts-type-expand-plugin",
  "version": "0.0.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc -p . --noEmit",
    "build": "run-s build:*",
    "build:types": "tsc -p .",
    "build:esbuild": "node ./bin/build.mjs",
    "prebuild": "rm -rf dist",
    "watch": "run-p watch:*",
    "watch:types": "pnpm build:types --watch",
    "watch:esbuild": "IS_WATCH=true pnpm build:esbuild",
    "lint:eslint": "eslint src --ext ts,js",
    "lint:prettier": "prettier '**/*.{ts,js,json,md}' --check",
    "lint": "run-p 'lint:*'",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "prettier '**/*.{ts,js,json,md}' --write",
    "fix": "run-p 'fix:*'",
    "prepublish": "pnpm build"
  },
  "dependencies": {
    "@trpc/server": "^10.38.3",
    "compiler-api-helper": "workspace:*",
    "express": "^4.18.2",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "esbuild": "^0.19.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "npm-run-all": "^4.1.5",
    "open-typescript": "^4.9.4",
    "prettier": "^3.0.3"
  }
}
