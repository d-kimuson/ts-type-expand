{
  "name": "ts-type-expand-plugin",
  "version": "0.0.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "typecheck": "tsc -p . --noEmit",
    "build": "run-s build:*",
    "build:types": "tsc -p .",
    "build:esbuild": "esno ./bin/build.ts",
    "prebuild": "rm -rf dist",
    "watch": "run-p watch:*",
    "watch:types": "yarn build:types --watch",
    "watch:esbuild": "IS_WATCH=true yarn build:esbuild",
    "prepublish": "yarn build"
  },
  "dependencies": {
    "compiler-api-helper": "^0.0.3",
    "express": "^4.17.3",
    "shared": "0.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "esbuild": "^0.14.36",
    "esno": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "open-typescript": "^4.5.0-dev.20210906"
  }
}
