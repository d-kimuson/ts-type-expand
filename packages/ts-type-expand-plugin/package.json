{
  "name": "ts-type-expand-plugin",
  "version": "0.0.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --clean --minify",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.mjs --no-error-on-unmatched-pattern --cache .",
    "fix": "pnpm lint --fix",
    "typecheck": "tsc -p . --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.2.0",
    "@trpc/server": "^10.9.0",
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "compiler-api-helper": "workspace:*",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-config": "workspace:*",
    "express": "^4.17.3",
    "globals": "^15.2.0",
    "tsup": "^8.0.2",
    "typescript-eslint": "^7.8.0",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  }
}
