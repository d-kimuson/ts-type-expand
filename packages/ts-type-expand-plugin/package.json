{
  "name": "ts-type-expand-plugin",
  "version": "0.0.2",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.cts"
    },
    "./server": {
      "require": "./dist/server/index.cjs",
      "types": "./dist/server/index.d.cts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --clean --minify",
    "lint": "eslint --no-error-on-unmatched-pattern --cache .",
    "fix": "pnpm lint --fix",
    "typecheck": "tsc -p . --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.4.0",
    "@trpc/server": "^10.45.2",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.12.0",
    "@volar/language-core": "^2.2.5",
    "@volar/source-map": "^2.2.5",
    "@volar/typescript": "^2.2.5",
    "@vue/language-core": "^2.0.19",
    "compiler-api-helper": "workspace:*",
    "eslint": "^9.4.0",
    "eslint-config": "workspace:*",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^4.0.0",
    "express": "^4.19.2",
    "globals": "^15.3.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  }
}
