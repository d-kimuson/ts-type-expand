{
  "name": "ts-type-expand-plugin",
  "version": "0.0.2",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "scripts": {
    "dev": "pnpm /^dev:.*/",
    "dev:types": "pnpm build:types --watch",
    "dev:esbuild": "IS_WATCH=true pnpm build:esbuild",
    "build": "pnpm /^build:.*/",
    "build:types": "tsc -p .",
    "build:esbuild": "node ./bin/build.mjs",
    "lint:eslint": "eslint src --ext ts,js",
    "lint:prettier": "prettier '**/*.{ts,js,json,md}' --check",
    "lint": "pnpm /^lint:.*/",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "prettier '**/*.{ts,js,json,md}' --write",
    "fix": "pnpm /^fix:.*/",
    "typecheck": "tsc -p . --noEmit",
    "prepublish": "pnpm build"
  },
  "dependencies": {
    "@trpc/server": "^10.9.0",
    "compiler-api-helper": "workspace:*",
    "express": "^4.17.3",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "esbuild": "^0.17.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3"
  }
}
