{
  "name": "compiler-api-helper",
  "version": "0.0.4",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:tsc": "pnpm build:tsc --watch",
    "dev:alias": "pnpm build:alias --watch",
    "typecheck": "tsc -p . --noEmit",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext ts",
    "fix": "run-s fix:*",
    "fix:eslint": "pnpm lint:eslint --fix",
    "build": "run-s build:*",
    "build:tsc": "tsc -p .",
    "build:alias": "tsc-alias -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "fp-ts": "^2.16.1",
    "ramda": "^0.29.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.2",
    "@types/ramda": "^0.29.4",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "esbuild": "^0.19.3",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postinstall-postinstall": "^2.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "tsd": "^0.29.0"
  }
}
