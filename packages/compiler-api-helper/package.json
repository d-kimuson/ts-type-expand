{
  "name": "compiler-api-helper",
  "version": "0.0.3",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify --clean",
    "lint": "eslint --no-error-on-unmatched-pattern --cache .",
