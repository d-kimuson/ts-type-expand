{
  "name": "compiler-api-helper",
  "version": "0.0.3",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:tsc": "yarn build:tsc --watch",
    "dev:alias": "yarn build:alias --watch",
    "typecheck": "tsc -p . --noEmit",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint . --ext ts",
    "fix": "run-s fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "build": "run-s build:*",
    "build:tsc": "tsc -p .",
    "build:alias": "tsc-alias -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "fp-ts": "^2.13.1",
    "ramda": "^0.27.1",
    "typescript": "^4.9.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/ramda": "^0.27.49",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "esbuild": "^0.17.2",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.3.1",
    "npm-run-all": "^4.1.5",
    "postinstall-postinstall": "^2.1.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.0",
    "tsconfig-paths": "^3.14.2",
    "tsd": "^0.18.0"
  }
}
