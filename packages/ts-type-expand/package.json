{
  "name": "ts-type-expand",
  "displayName": "ts-type-expand",
  "description": "vscode extension for expand type of typescript",
  "version": "1.0.6",
  "license": "MIT",
  "publisher": "kimuson",
  "repository": {
    "type": "git",
    "url": "https://github.com/d-kimuson/ts-type-expand"
  },
  "keywords": [
    "typescript",
    "ts",
    "compilerAPI",
    "expand",
    "tree"
  ],
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onView:typeExpand"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Ts Type Expand",
      "type": "object",
      "properties": {
        "ts-type-expand.validate": {
          "type": "array",
          "items": {
            "anyOf": [
              {
                "type": "string"
              }
            ]
          },
          "description": "Validate by specifying languageId",
          "default": [
            "typescript",
            "typescriptreact",
            "javascript",
            "javascriptreact"
          ]
        },
        "ts-type-expand.compactOptionalType": {
          "type": "boolean",
          "default": true,
          "description": "Fold optional type"
        },
        "ts-type-expand.compactPropertyLength": {
          "type": "number",
          "default": 80,
          "description": "Display {...} if property type text is longer than this length"
        },
        "ts-type-expand.directExpandArray": {
          "type": "boolean",
          "default": true,
          "description": "Directly expand T for Array[T]"
        },
        "ts-type-expand.port": {
          "type": "number",
          "default": 54334,
          "description": "Port used for communication with TS server."
        }
      }
    },
    "commands": [
      {
        "command": "ts-type-expand.restart",
        "title": "Restart (ts-type-expand)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "typeExpand",
          "name": "TS Type Expand"
        }
      ]
    },
    "typescriptServerPlugins": [
      {
        "name": "ts-type-expand-plugin",
        "enableForWorkspaceTypeScriptVersions": true
      }
    ]
  },
  "extensionDependencies": [
    "vscode.typescript-language-features"
  ],
  "scripts": {
    "dev": "tsup --watch --clean=false --minify=false",
    "build": "tsup",
    "typecheck": "tsc --noEmit -p .",
    "lint": "eslint src --ext ts,js",
    "fix": "pnpm lint:eslint --fix",
    "test": "jest",
    "test:vscode": "node ./dist/test/vscode/runTest.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@trpc/server": "^10.9.0",
    "@types/axios": "^0.14.0",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.11",
    "@types/vscode": "1.62",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@trpc/client": "^10.9.0",
    "axios": "^0.26.1",
    "compiler-api-helper": "workspace:*",
    "get-port": "^6.1.2",
    "node-fetch": "^3.3.0",
    "ts-type-expand-plugin": "workspace:*",
    "typescript": "^4.9.4",
    "winston": "^3.8.2",
    "winston-transport": "^4.7.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "glob": "^7.2.0",
    "jest": "^27.4.3",
    "mocha": "^9.1.3",
    "ts-jest": "^27.0.7",
    "tsup": "^8.0.2",
    "vscode-test": "^1.6.1"
  }
}
