{
  "name": "ts-type-expand",
  "displayName": "ts-type-expand",
  "description": "expand type for typescript",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ts-type-expand.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ts-type-expand.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "ts:run": "ts-node -P ./tsconfig.json",
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "typecheck": "tsc --noEmit -p .",
    "lint": "yarn typecheck && run-p lint:*",
    "lint:eslint": "eslint src --ext ts,js",
    "lint:prettier": "prettier '**/*.{ts,js,json,md}' --check",
    "fix": "run-p fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "prettier '**/*.{ts,js,json,md}' --write",
    "pretest": "yarn run compile && yarn run lint",
    "test:vscode": "node ./out/test/runTest.js",
    "test:jest": "jest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.23",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mocha": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "vscode-test": "^1.5.0",
    "husky": "^6.0.0"
  },
  "dependencies": {
    "typescript": "^4.2.4"
  }
}
